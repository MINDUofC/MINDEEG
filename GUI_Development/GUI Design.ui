     <string notr="true">/* === Taskbar Menu Dropdown === */
QComboBox#MenuOptions {
    font-family: 'Montserrat Medium';
    font-size: 14px;
    color: black;
    background-color: #cfcfcf; /* Matches taskbar background */
    border: 2px solid #afafaf;
    border-radius: 0px;
    padding: 3px;
    min-width: 100px; /* Ensure size consistency */
	


}

/* Set &quot;Menu&quot; as the default display text */
QComboBox#MenuOptions::before {
    color: black;
}

/* Dropdown arrow */
QComboBox#MenuOptions::drop-down {
	image:url(:/images/down_arrow.png);
	padding: 7px;
    border: none;
    background: #cfcfcf; 
    width: 20px;
    border-left: 2px solid #afafaf; /* Separates dropdown arrow */
}




QComboBox#MenuOptions::drop-down:on { /* Arrow effect when clicked */
    image: url(:/images/up_arrow.png);
}


/* Hover effect */
QComboBox#MenuOptions:hover {
    background-color: #b0b0b0;
}

/* Dropdown popup styling */
QComboBox#MenuOptions QAbstractItemView {
    background-color: white;
    border: 1px solid #ccc;
	border-radius:3px;
    selection-background-color: #a3c1e1;
    padding: 5px;
}

/* Dropdown list items */
QComboBox#MenuOptions QAbstractItemView::item {
    font-family: 'Montserrat Medium';
    font-size: 14px;
    padding: 5px;
    color: #333;
}

/* Highlight dropdown items on hover */
QComboBox#MenuOptions QAbstractItemView::item:hover {
    background-color: #e0e0e0;
    color: black;
}

/* Highlight when selected */
QComboBox#MenuOptions QAbstractItemView::item:selected {
    background-color: #a3c1e1;
    color: black;
}
