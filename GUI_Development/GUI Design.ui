      <property name="styleSheet">
       <string notr="true">QWidget {
    font-family: &quot;Montserrat SemiBold&quot;;
    font-size: 11px;
    padding: 0px; /* No extra padding */
}


/* === QTabWidget Pane (The Area Below Tabs) === */
QTabWidget::pane {
    border: 2px solid black; /* Black outline */
    border-radius: 5px; /* Rounded corners */
    background-color: white; /* Keep pane background white */
}

/* === QTabBar (The Row of Tabs) === */
QTabBar {
    font-family: &quot;Montserrat SemiBold&quot;;
    font-size: 11px;
}

/* === Individual Tabs === */
QTabBar::tab {
    width: 80px;
    padding: 6px 12px;  /* Controls tab height &amp; width */	
	margin-left:3px;
    margin-right: 5px;  /* Space between tabs */
    background-color: white; /* Default tab background */
    border: 3px solid #ccc; /* Subtle border */
    border-top-left-radius: 5px;
    border-top-right-radius: 5px;
    transition: all 0.2s ease-in-out; /* Smooth transition */
}

/* === Hover Effect on Tabs === */
QTabBar::tab:hover {
    background-color: #e0e0e0; /* Light grey background on hover */
    border: 3px solid #0047B2; /* Bold blue border */
    color: #0047B2; /* Change text color */
}

/* === Selected Tab (Active) === */
QTabBar::tab:selected {
    background-color: #f5f5f5; /* Highlight selected tab */
    border: 3px solid black;
    border-bottom: none; /* Merges with the screen below */
    font-family: &quot;Montserrat Black&quot;;
}

/* === Pressed Effect (Shrinks Tab Slightly) === */
QTabBar::tab:pressed {
    transform: scale(0.95); /* Shrinks tab slightly */
}
